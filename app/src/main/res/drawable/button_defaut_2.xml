<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <stroke android:color="@color/color_title" android:width="@dimen/dp_1"/>
            <solid android:color="@color/gray_f3f3f3"/>
            <padding android:bottom="@dimen/dp_15" android:left="@dimen/dp_15"
                 android:top="@dimen/dp_15" android:right="@dimen/dp_15"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <stroke android:color="@color/color_title" android:width="@dimen/dp_1"/>
            <solid android:color="@color/white"/>
            <padding android:bottom="@dimen/dp_15" android:left="@dimen/dp_15"
                android:top="@dimen/dp_15" android:right="@dimen/dp_15"/>
        </shape>
    </item>
</selector>