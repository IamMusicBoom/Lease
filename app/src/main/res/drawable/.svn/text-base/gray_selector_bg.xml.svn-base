<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_pressed="false">
    <shape android:shape="rectangle">
        <corners android:radius="@dimen/dp_5"/>
        <stroke android:width="@dimen/dp_1" android:color="@color/gray_333333"/>
        <padding android:bottom="@dimen/dp_10" android:top="@dimen/dp_10"
            android:left="@dimen/dp_15" android:right="@dimen/dp_15"/>
        <solid android:color="@color/white"/>
    </shape>
</item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dp_5"/>
            <stroke android:width="@dimen/dp_1" android:color="@color/gray_333333"/>
            <padding android:bottom="@dimen/dp_10" android:top="@dimen/dp_10"
                android:left="@dimen/dp_15" android:right="@dimen/dp_15"/>
            <solid android:color="@color/white_click_4effffff"/>
        </shape>
    </item>
</selector>